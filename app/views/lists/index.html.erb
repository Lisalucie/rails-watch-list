<br>
<h1>Show all my lists</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @lists.each do |list| %>
      <tr>
      <td><%= list.name %></td>
      <td><%= link_to 'Show',list, class: "btn btn-primary" %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<%= link_to 'Add a list',new_list_path, class: "btn btn-primary" %>
